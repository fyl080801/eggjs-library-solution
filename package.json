{
  "name": "eggjs-library-solution",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "egg": {
    "declarations": false
  },
  "dependencies": {
    "egg": "^2.33.1",
    "egg-scripts": "^2.15.2"
  },
  "devDependencies": {
    "autod": "^3.1.2",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.17.0",
    "egg-ci": "^1.19.0",
    "egg-mock": "^4.2.0",
    "eslint": "7.12.1",
    "eslint-config-egg": "^9.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "lerna run clean --parallel && rimraf dist",
    "build": "lerna run build",
    "start": "egg-scripts start --daemon --title=egg-server-eggjs-test1",
    "stop": "egg-scripts stop --title=egg-server-eggjs-test1",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug --env=prod",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "12"
  },
